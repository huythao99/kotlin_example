<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".PhysicalMeasuresActivity"
            android:exported="false"
            android:label="@string/title_activity_physical_measures"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity
            android:name=".CompleteActivity"
            android:exported="false" />
        <activity
            android:name=".PhysicalExerciseActivity"
            android:exported="false"
            android:label="@string/title_activity_physical_exercise"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity
            android:name=".ExerciseActivity"
            android:exported="false"
            android:label="@string/title_activity_exercise"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity
            android:name=".ResultActivity"
            android:exported="false"
            android:label="@string/title_activity_result"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.MyApplication.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScanActivity"
            android:exported="true"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
    </application>

</manifest>